{
  "name": "ets-log210-typescript-node-api",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "startWithDoc": "npm run compile && npm run all_docs && node dist/index.js",
    "start": "npm run compile && node dist/index.js",
    "start-docker": "apidoc -i src/routes/ -o dist/docs/ && node dist/index.js",
    "test": "jest --colors",
    "watch": "jest --coverage --watchAll",
    "all_docs": "npm run docs && npm run madge && npm run tplant",
    "docs": "apidoc -i src/routes/ -o dist/docs ",
    "circular": "madge src/App.ts -c",
    "madge": "madge src/App.ts -i madge.png && cp madge.png dist/docs/",
    "tplant": "tplant --input src/App.ts  -A --output dist/docs/dcl.svg && cp src/model/model/MDD.svg dist/docs/",
    "precompile": "npm run clean",
    "clean": "rimraf dist/controller && rimraf dist/model && rimraf dist/routes && rimraf dist/App.js && rimraf dist/index.js",
    "compile": "npm run compile:ts",
    "compile:ts": "tsc --outDir ./dist --module commonjs --downlevelIteration ./src/index.ts",
    "copydata": "cp -R src/data dist/data"
  },
  "watch": {
    "test": "{src,tests}/*.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@types/chai": "^4.3.16",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/md5": "^2.3.5",
    "@types/morgan": "^1.9.9",
    "@types/multimap": "^1.1.4",
    "@types/supertest": "^6.0.2",
    "apidoc": "^1.2.0",
    "chai-http": "^4.4.0",
    "coveralls": "^3.1.1",
    "debug": "^4.3.4",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "madge": "^7.0.0",
    "npm-watch": "^0.13.0",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.7",
    "supertest": "^7.0.0",
    "system-sleep": "^1.3.7",
    "testdouble": "^3.20.2",
    "testdouble-chai": "^0.5.0",
    "ts-jest": "^29.1.4",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "express": "^4.19.2",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "multimap": "^1.1.0",
    "tplant": "^3.1.0"
  }
}
